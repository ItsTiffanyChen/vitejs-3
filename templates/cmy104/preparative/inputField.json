{
  "familyName": {
    "field": "姓氏",
    "regex": "^[^!@#$%^&*+=_{}|\\\\:><?\/;[\\]~`,0-9\uff01\uff20\uff03\uff04\uff05\ufe3f\uff06\uff0a\uff0b\uff1d\ufe4d\uff5b\uff5d\uff5c\uff3c\uff07\uff1a\uff1e\uff1c\uff1f\uff0c\uff0f\uff1b\uff3b\uff3d\uff5e\uff40\uff10\uff11\uff12\uff13\uff14\uff15\uff16\uff17\uff18\uff19]+$",
    "maxLength": 30
  },
  "firstName": {
    "field": "名字",
    "regex": "^[^!@#$%^&*+=_{}|\\\\:><?\/;[\\]~`,0-9\uff01\uff20\uff03\uff04\uff05\ufe3f\uff06\uff0a\uff0b\uff1d\ufe4d\uff5b\uff5d\uff5c\uff3c\uff07\uff1a\uff1e\uff1c\uff1f\uff0c\uff0f\uff1b\uff3b\uff3d\uff5e\uff40\uff10\uff11\uff12\uff13\uff14\uff15\uff16\uff17\uff18\uff19]+$",
    "maxLength": 30,
    "blackList":[
      "\u5148\u751f",
      "\u5c0f\u59d0",
      "\u5c0f\u59ca"
    ]
  },
  "identity": {
    "twId": {
        "field": "身分證字號",
        "regex": "^[a-zA-Z]{1}[12]{1}[\\d]{8}$"
    },
    "passport": {
        "field": "護照號碼",
        "regex": "(?!^[A-z]+$)^([A-z0-9]){0,14}$",
        "maxLength": 15
    }
  },
  "birthYear": {
    "field": "出生年份",
    "regex": "^\\d{4}$",
    "min": 1940,
    "maxLength": 4
  },
  "cellphone": {
    "field": "手機號碼",
    "regex": "^\\+?[0-9]*$",
    "minLength": 10,
    "maxLength": 15
  },
  "password": {
    "field": "密碼",
    "regex": "^(?=.*\\d)(?=.*[a-zA-Z])[a-zA-Z0-9]{6,12}$",
    "minLength": 6,
    "maxLength": 12
  },
  "email": {
    "field": "E-mail",
    "regex": "^[a-zA-Z0-9_.-]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)*(\\.[a-zA-Z]{2,})$",
    "maxLength": 400
  }
}