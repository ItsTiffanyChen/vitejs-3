{
  "familyName": {
    "field": "姓氏",
    "regex": "^[a-zA-Z_\\.\\(\\)\\-（） 「」\\u4e00-\\u9fa5]+$",
    "maxLength": 30
  },
  "firstName": {
    "field": "名字",
    "regex": "^[a-zA-Z_\\.\\(\\)\\-（） 「」\\u4e00-\\u9fa5]+$",
    "maxLength": 30
  },
  "identity": {
    "twId": {
        "field": "身分證字號",
        "regex": "^[a-zA-Z]{1}[12]{1}[\\d]{8}$"
    },
    "passport": {
        "field": "護照號碼",
        "regex": "(?!^[A-z]+$)^([A-z0-9]){0,14}$",
        "maxLength": 15
    }
  },
  "birthYear": {
    "field": "出生年份",
    "regex": "^\\d{4}$",
    "min": 1940,
    "maxLength": 4
  },
  "cellphone": {
    "field": "手機號碼",
    "regex": "^\\+?[0-9]*$",
    "minLength": 10,
    "maxLength": 15
  }
}